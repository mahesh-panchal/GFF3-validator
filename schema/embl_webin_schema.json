{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "seqname": { "type": "string" },
    "source": { "type": "string" },
    "feature": { "type": "string" },
    "start": { "type": "integer" },
    "end": { "type": "integer" },
    "score": { "type": ["number", "null"] },
    "strand": {
      "type": "string",
      "emum": ["+", "-", "?"]
    },
    "phase": {
      "type": "integer",
      "enum": [0, 1, 2]
    },
    "attributes": { "type": "object" }
  },
  "required": ["seqname", "source", "feature", "start", "end"]
}
