{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "seqname": { "type": "string" },
    "source": { "type": "string" },
    "feature": {
      "type": "string",
      "description": "The key of the feature, e.g., 'gene', 'CDS', etc.",
      "enum": [
        "assembly_gap",
        "C_region",
        "CDS",
        "centromere",
        "D-loop",
        "D_segment",
        "exon",
        "gap",
        "gene",
        "iDNA",
        "intron",
        "J_segment",
        "mat_peptide",
        "misc_binding",
        "misc_difference",
        "misc_feature",
        "misc_recomb",
        "misc_RNA",
        "misc_structure",
        "mobile_element",
        "modified_base",
        "mRNA",
        "ncRNA",
        "N_region",
        "old_sequence",
        "operon",
        "oriT",
        "polyA_site",
        "precursor_RNA",
        "prim_transcript",
        "primer_bind",
        "propeptide",
        "protein_bind",
        "regulatory",
        "repeat_region",
        "rep_origin",
        "rRNA",
        "S_region",
        "sig_peptide",
        "source",
        "stem_loop",
        "STS",
        "telomere",
        "tmRNA",
        "transit_peptide",
        "tRNA",
        "unsure",
        "V_region",
        "V_segment",
        "variation",
        "3'UTR",
        "5'UTR"
      ]
    },
    "start": { "type": "integer" },
    "end": { "type": "integer" },
    "score": { "type": ["number", "null"] },
    "strand": {
      "type": "string",
      "emum": ["+", "-", "?"]
    },
    "phase": {
      "type": "integer",
      "enum": [0, 1, 2]
    },
    "attributes": { "type": "object" }
  },
  "required": ["seqname", "source", "feature", "start", "end"]
}
